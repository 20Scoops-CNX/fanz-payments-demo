{
  "name": "fanz-payments-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "final-form": "^4.18.4",
    "helmet": "^4.2.0",
    "i18next": "^17.0.18",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "qs": "^6.8.0",
    "ramda": "^0.27.1",
    "react": "^16.10.2",
    "react-dom": "^16.11.0",
    "react-final-form": "^6.3.0",
    "react-ga": "^3.3.0",
    "react-i18next": "^11.0.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.6.3",
    "styled-components": "^5.3.3",
    "styled-media-query": "^2.1.2",
    "warning": "^4.0.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "CI=false react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint:check": "eslint src --color",
    "lint:format": "eslint src --fix",
    "stylelint:check": "stylelint './src/**/*.js'",
    "storybook": "start-storybook -p 6006 -c ./.storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier --write 'src/**/*.{js,css}'",
    "prettier:check": "prettier --list-different 'src/**/*.{js,css}'",
    "prettier-changefiles": "pretty-quick --staged",
    "docker:add": "docker-compose exec -w /opt/node_app frontend_dev yarn add",
    "dev": "concurrently -n watch-frontend,watch-storybook -c blue,green \"yarn start\" \"yarn storybook\"",
    "check-vulnerabilities": "retire --outputformat json --outputpath artifacts/result-vulnerabilities.json",
    "check-bundle-size": "yarn build && size-limit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier-changefiles"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/locales/**",
      "!src/utils/test/**"
    ],
    "coverageReporters": [
      "html"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-app": "^2.1.6",
    "@size-limit/webpack": "^2.1.6",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^9.3.2",
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^3.1.0",
    "jest-html-reporters": "^2.1.2",
    "jest-in-case": "^1.0.2",
    "jest-localstorage-mock": "^2.4.2",
    "jest-mock-console": "^1.0.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.8",
    "retire": "^2.2.3",
    "storybook-react-router": "^1.0.7",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.8.0",
    "testing-library": "^0.0.2",
    "yargs": "^15.3.1"
  }
}
